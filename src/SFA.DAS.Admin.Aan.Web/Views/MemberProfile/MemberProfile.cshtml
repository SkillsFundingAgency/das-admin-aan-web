@model AmbassadorProfileViewModel
@{
    ViewData["Title"] = "Member Profile";
}

<partial name="_validationSummary" />
<div class="govuk-grid-row">
    <div class="govuk-grid-column-two-thirds">
        @{
            await Html.RenderPartialAsync("~/Views/MemberProfile/_MemberPersonalInfo.cshtml", Model.MemberInformation);

            await Html.RenderPartialAsync("~/Views/MemberProfile/_AreasOfInterest.cshtml", Model.AreasOfInterest);

            await Html.RenderPartialAsync("~/Views/MemberProfile/_EmployerInformationSection.cshtml", Model.ApprenticeshipInformation);
            await Html.RenderPartialAsync("~/Views/MemberProfile/_ApprenticeInformationSection.cshtml", Model.ApprenticeshipInformation);

            await Html.RenderPartialAsync("~/Views/MemberProfile/_LinkedInProfile.cshtml", Model.ConnectViaLinkedIn);
        }
    </div>
</div>
